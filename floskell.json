{
    "language": "Haskell2010",
    "extensions": [
        "OverloadedStrings",
        "NamedFieldPuns",
        "LambdaCase",
        "RankNTypes",
        "InstanceSigs",
        "DefaultSignatures",
        "GeneralizedNewtypeDeriving",
        "StandaloneDeriving",
        "MultiParamTypeClasses",
        "ScopedTypeVariables",
        "MonoLocalBinds",
        "DataKinds",
        "KindSignatures",
        "FlexibleInstances",
        "FlexibleContexts",
        "UndecidableInstances",
        "TypeFamilies",
        "GADTs",
        "CPP"
    ],
    "style": "johan-tibell",
    "fixities": [
        "infixr 0 $#",
        "infixr 9 .#",
        "infixr 6 <~>",
        "infixr 6 <^>",
        "infixr 6 <=>",
        "infixr 6 <->",
        "infixr 6 <|>",
        "infixr 6 <:>",
        "infixr 6 <!>",
        "infixr 6 <Â°>",
        "infixr 6 <\\>",
        "infixr 6 <#>"
    ],
    "formatting": {
        "penalty": {
            "max-line-length": 120
        },
        "options": {
            "sort-imports": [
                {
                    "prefixes": [""],
                    "order": "grouped"
                },
                {
                    "prefixes": ["Zug"],
                    "order": "grouped"
                }
            ]
        }
    }
}