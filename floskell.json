{
    "language": "Haskell2010",
    "extensions": [
        "OverloadedStrings",
        "NamedFieldPuns",
        "LambdaCase",
        "RankNTypes",
        "InstanceSigs",
        "DefaultSignatures",
        "GeneralizedNewtypeDeriving",
        "StandaloneDeriving",
        "MultiParamTypeClasses",
        "ScopedTypeVariables",
        "MonoLocalBinds",
        "DataKinds",
        "KindSignatures",
        "FlexibleInstances",
        "FlexibleContexts",
        "UndecidableInstances",
        "TypeFamilies",
        "GADTs",
        "ViewPatterns",
        "TemplateHaskell",
        "CPP"
    ],
    "style": "johan-tibell",
    "fixities": [
        "infixr 0 $#",
        "infixr 9 .#",
        "infixr 6 <~>",
        "infixr 6 <^>",
        "infixr 6 <=>",
        "infixr 6 <->",
        "infixr 6 <|>",
        "infixr 6 <:>",
        "infixr 6 <!>",
        "infixr 6 <Â°>",
        "infixr 6 <\\>",
        "infixr 6 <#>"
    ],
    "formatting": {
        "penalty": {
            "max-line-length": 100
        },
        "options": {
            "sort-imports": [
                {
                    "prefixes": [
                        ""
                    ],
                    "order": "sorted"
                },
                {
                    "prefixes": [
                        "Zug"
                    ],
                    "order": "sorted"
                }
            ]
        },
        "layout": {
            "record": "try-oneline"
        },
        "indent": {
            "where": 4,
            "where-binds": "indent-by 4",
            "multi-if": "indent-by 4"
        },
        "group": {
            "(": {
                "spaces": "none",
                "linebreaks": "after",
                "force-linebreak": false
            },
            ")": {
                "spaces": "none",
                "linebreaks": "after",
                "force-linebreak": false
            }
        },
        "op": {
            ",": {
                "spaces": "after",
                "linebreaks": "before",
                "force-linebreak": false
            },
            ", in declaration": {
                "spaces": "after",
                "linebreaks": "before",
                "force-linebreak": false
            },
            ", in type": {
                "spaces": "after",
                "linebreaks": "before",
                "force-linebreak": false
            },
            ", in pattern": {
                "spaces": "after",
                "linebreaks": "before",
                "force-linebreak": false
            },
            ", in other": {
                "spaces": "after",
                "linebreaks": "before",
                "force-linebreak": false
            },
            "@": {
                "spaces": "none",
                "linebreaks": "none",
                "force-linebreak": false
            },
            "<-": {
                "spaces": "both",
                "linebreaks": "before",
                "force-linebreak": false
            },
            "->": {
                "spaces": "both",
                "linebreaks": "before",
                "force-linebreak": false
            }
        }
    },
    "options": {
        "align-sum-type-decl": true,
        "sort-import-lists": true,
        "sort-imports": true,
        "sort-pragmas": true,
        "split-language-pragmas": true,
        "flexible-oneline": true
    }
}